{
  "compilerOptions": {
    "target": "ESNEXT",
    "module": "commonjs",
    "lib": [
      "es6",
    ],
    "declaration": false,
    "sourceMap": false,
    "outDir": "./dist",
    "noImplicitAny": true,
    "moduleResolution": "Node16",
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": false,
    "types": [
      "jest",
      "node"
    ],
    "paths": {
      "@config/*": ["./config/*"],
      "@api/*": ["./api/*"],
      "@utils/*": ["./utils/*"],
      "@middle/*": ["./middle/*"],
    }
  },
  "include": [
      "./*.ts",
      "./**/*.ts",
  ],
  "exclude": [
      "node_modules"
    ],
}